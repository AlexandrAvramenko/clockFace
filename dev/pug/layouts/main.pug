block variables

doctype html
html(lang="en")
	head
		if title
			title #{title}
		meta(charset="utf-8")
		meta(http-equiv='X-UA-Compatible', content="IE=edge,chrome=1")
		meta(name='viewport' content='width=device-width, initial-scale=1.0, maximum-scale=2.0')
		link(rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.min.css")
		link(rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick-theme.min.css")
		link(rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/smoothness/jquery-ui.css")
		link(rel="stylesheet" href="css/styles.min.css")
	body
		include ../includes/header
		.content
			block content
		include ../includes/footer
	block script
		script(src='https://code.jquery.com/jquery-3.5.1.min.js')
		script(src='https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.min.js')
		script(src="https://code.jquery.com/ui/1.12.1/jquery-ui.js")
		script(async src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC7g1POKIQj601kBUvNOOiInc-asxDJxSw&callback=initMap")
		script(src="js/price-range.min.js")
		script(src="js/scrollbar.min.js")
		script(src="js/bundle.min.js")
